#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/dist/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/dist/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/tsx@4.19.3/node_modules:/mnt/c/Users/sgparodil/OneDrive - Asociación Chilena de Seguridad/Escritorio/Proyect/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tsx@4.19.3/node_modules/tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../tsx@4.19.3/node_modules/tsx/dist/cli.mjs" "$@"
fi
